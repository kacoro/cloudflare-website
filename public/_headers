# https://developers.cloudflare.com/workers/static-assets/headers
# 图片资源缓存
/images/*
  cache-control: public, max-age=31536000, immutable
  expires: Thu, 31 Dec 2037 23:55:55 GMT

# 字体资源缓存
/fonts/*
  cache-control: public, max-age=31536000, immutable

# CSS/JS 资源缓存
/_next/static/*
  Cache-Control: public,max-age=31536000,immutable
