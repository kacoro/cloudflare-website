// wrangler.test.jsonc
{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "areaflysolar-test",
  "main": ".open-next/worker.js",
  "compatibility_date": "2025-03-01",
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
  "workers_dev": false,        // 生产环境应设为 false
  "preview_urls": false,       // 生产环境建议设为 false
 
  "routes": [
    {
      "pattern": "test.areaflysolar.com",
      "zone_name": "areaflysolar.com",
      "custom_domain": true
    }
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": ".open-next/assets"
  },
  // "r2_buckets": [
  //   {
  //     "binding": "MY_BUCKET",
  //     "bucket_name": "areafly-test"
  //   }
  // ],
  "observability": {
    "enabled": true
  },
  "vars": {
    "NEXTJS_ENV": "production",
    "NEXT_PUBLIC_BASE_URL": "https://test.areaflysolar.com",
    "NODE_ENV": "production"
  }
}